{
  "name": "home-core",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:web": "pnpm --filter @home/web dev",
    "dev:api": "pnpm --filter @home/api dev",
    "build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write .",
    "changeset": "changeset",
    "release": "changeset publish",
    "start:web": "pnpm --filter @home/web build && pnpm --filter @home/web preview",
    "start:api": "pnpm --filter @home/api build && pnpm --filter @home/api start"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@changesets/cli": "^2.27.8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "turbo": "^2.1.1",
    "typescript-eslint": "^8.8.1",
    "typescript": "^5.6.2"
  }
}
